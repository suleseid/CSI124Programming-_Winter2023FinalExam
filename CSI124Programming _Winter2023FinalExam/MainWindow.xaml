<Window x:Class="CSI124Programming__Winter2023FinalExam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSI124Programming__Winter2023FinalExam"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="145,53,139,54" Background="#FFB0A7A7">
        <Canvas >
            <Label x:Name="Password" Content="Password" Width="87" Canvas.Left="76" Canvas.Top="122"/>
            <PasswordBox x:Name="psPassword" Canvas.Left="181" Canvas.Top="122" Width="248" Height="26"/>
            <Label x:Name="lbUserName" Content="UserName" HorizontalAlignment="Left" Margin="75,68,0,0" VerticalAlignment="Top" Width="87"/>
            <TextBox x:Name="txtUserName" Canvas.Left="185" TextWrapping="Wrap" Canvas.Top="68" Width="244" Height="26"/>
            <Button x:Name="btnLogin" Content="Login" Canvas.Left="230" Canvas.Top="197" Width="142" Height="24" Click="btnLogin_Click"/>
        </Canvas>

    </Grid>
</Window>
